page{background: #f3f5f7}
.header{padding: 50rpx 0;width: 100%;background: #e85654;text-align: center;}
.header .thumb{width: 160rpx;height: 160rpx;border-radius:50%; }
.header-info{padding-top: 10rpx;}
.header-info text {
  width: 100%;
  align-items: flex-start;
  text-align: center;font-size: 34rpx;color: #333;
}
.my-item{width: 100%;border-bottom: 1px solid #eee;display: inline-block; background: url(https://cdn.it120.cc/images/weappshop/arrow-right.png) no-repeat 705rpx center;background-size: 15rpx auto;background-color: #fff;}
.my-item navigator{width: 690rpx;padding: 30rpx;display: inline-block;color: #333;}
.my-item navigator{display: flex;justify-content:space-between;}
.my-item navigator .t1{line-height: 52rpx;font-size: 32rpx}
.my-item navigator .t2{font-size: 28rpx;color: #999;line-height: 54rpx;display: block;margin-right: 30rpx;}
.kongbai{width: 100%;height: 20rpx;background: #efeff4;}
.list{width: 100%;height: 145rpx;background: #fff;display:flex;}
.li{flex: 1;height: 100%;}
.li .img{width: 100%;height: 50rpx; padding:35rpx 0 0 0; }
.li image{width: 50rpx;height: 50rpx;}
.li .txt{}
.li text{font-size: 26rpx;color: #333333;display: inline-block;height: 30rpx;line-height: 30rpx;}
.li view{width: 100%;text-align: center;}

